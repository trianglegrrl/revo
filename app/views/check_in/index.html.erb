<div class="alert alert-primary" role="alert" id="notice">
	<%= notice %>
</div>
<div class="checkin-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
      <h1 class="display-3">Check In Now</h1>
      <p class="lead">We're glad you're here. Let's get to work.</p>
    </div>

<table class="table">
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="check_in_user_list">
					<h3 class="user mx-auto display-4" style="width:400px">
						<%= link_to user.full_name, user_user_action_steps_path(user) %>
					</h3>
				</td>
      </tr>
    <% end %>
  </tbody>
</table>
